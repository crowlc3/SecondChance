{
	"manifest_version": 2,

	"name": "Second Chance",
	"description": "SD&D",
	"version": "1.0",

	"browser_action": {
	"default_icon": "icon.png",
	"default_popup": "popup.html"
	},
	"permissions":[
         "<all_urls>",
         "webRequest",
	 "webRequestBlocking"    
	],
	"background": {
	"scripts": ["detect.js", "catch.js"],
	"persistent": true
	}
}
